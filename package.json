{
  "name": "ughlangsupport",
  "displayName": "UghLangSupport",
  "description": "Keyword highlighting for Ugh",
  "version": "0.0.2",
  "publisher": "BiznesBear",
  "repository": {"url": "https://github.com/BiznesBear/ughlangsupport"},
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "configuration": {
    "type": "object",
    "title": "Example configuration",
    "properties": {
        "languageServerExample.maxNumberOfProblems": {
            "scope": "resource",
            "type": "number",
            "default": 100,
            "description": "Controls the maximum number of problems produced by the server."
        }
    }
  },
  "contributes": {
    "languages": [{
      "id": "ugh",
      "aliases": ["ugh", "ugh"],
      "extensions": ["ugh"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "ugh",
      "scopeName": "source.ugh",
      "path": "./syntaxes/ugh.tmLanguage.json"
    }]
  }
}
